[run]
branch = True

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise ValueError
    raise NotImplementedError
    raise ImportError
    except ImportError:
    except ModuleNotFoundError:
    except IndexError:
    except AttributeError:
    except KeyError:
    except ValueError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    tests/*
